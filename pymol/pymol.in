#!/bin/sh
PYMOL_VERSION=@PYMOL_VERSION@
PYTHON=@PYTHON@
PYTHON_VERSION=@PYTHON_VERSION@
libdir=@libdir@

if test ${PYMOL_VERSION} -lt 3900 ; then
  init=${libdir}/pymol/modules/launch_pymol.py
else
  init=${libdir}/python${PYTHON_VERSION}/site-packages/pymol/__init__.py
fi
exec $PYTHON ${init} "$@"
